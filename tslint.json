{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules",
    "tslint-immutable",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": ["never-prefix"],
    "variable-name": [true, "allow-leading-underscore", "ban-keywords"]
  },
  "rulesDirectory": []
}
